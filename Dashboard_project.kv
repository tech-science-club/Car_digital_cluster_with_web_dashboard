<Sport_style_dash_board>:
	canvas.before:
		Color:
		    rgba: 0, 0, 0, 1  # red
		Ellipse:
		    pos: self.x+300, self.y-75
		    size: 600, 600
        Color:
            rgba: 0+root.x_rpm_scale/240, 1-root.x_rpm_scale/240, 0, 1  # red
        Ellipse:
            pos: self.x+300, self.y-75
            size: 600, 600
            angle_start: 237
            angle_end: 237+root.x_rpm_scale

        Color:
            rgba: 0, 0, 0, 1
        Ellipse:
            pos: self.x+350, self.y-25
            size: 500, 500

        Color:
            rgba: 1,1,1,1
        Rectangle:
            pos: self.x+375, self.y
            size: 450, 450
            source: "central_ring2.png"
        Color:
            rgba: 1,1,1,1
        Rectangle:
            pos: 528, 310
            size: 155, 67
            source: "rpm_label.png"
	canvas:
		Color:
		    rgba: 1, 1, 1, 1  # red
		Rectangle:
		    pos: self.x+300, self.y -75
		    size: 600, 600
		    source: "rpm.png"

	MDLabel:
		id: speed
		pos_hint: {"center_x": 0.5, "center_y": 0.3}
		#pos: 500, 250
		halign:'center'
		text: "000"
		font_name: 'TickingTimebombBB'
		font_size: 170
		markup: True

	MDLabel:

		text: "km/h"

		font_size: 40
        pos_hint: {"center_x": 0.58, "center_y": 0.2}
		#pos: 600, 220
		font_name: 'TickingTimebombBB'
		halign:'center'

	MDLabel:
		id: rpm
		pos_hint: {"center_x": 0.5, "center_y": 0.5}
		halign:'center'
		text: "0000"
		font_name: 'TickingTimebombBB'
		font_size: 50
		markup: True

	MDLabel:
		pos_hint: {"center_x": 0.55, "center_y": 0.46}
		halign:'center'
		text: "RPM"
		font_name: 'TickingTimebombBB'
		font_size: 32
		markup: True


